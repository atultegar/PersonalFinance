        Title="main" Height="300" Width="300" Background="#FF4D6082">
    

    <StackPanel Background="#FF293956">
        <ComboBox x:Name="ComboBox1" Margin="10" Width="250" HorizontalAlignment="Center" SelectionChanged="ComboBox1_SelectionChanged" SelectedIndex="0"/>
        <ComboBox x:Name="ComboBox2" Margin="10" Width="250" HorizontalAlignment="Center" SelectionChanged="ComboBox1_SelectionChanged" SelectedIndex="0"/>
        <ComboBox x:Name="ComboBox3" Margin="10" Width="250" HorizontalAlignment="Center" SelectionChanged="ComboBox3_SelectionChanged"/>
        
        <TextBox x:Name="Textbox1" Margin="10" Width="250"/>
        <TextBox x:Name="TextBox2" Margin="10" Width="250" />
    </StackPanel>
    
</Window>
